{
  "name": "tokens-builder",
  "version": "1.0.1",
  "description": "tokens-builder",
  "scripts": {
    "build": "node config.js",
    "transform": "npm run sd-themes && npm run sd-fonts",
    "sd-fonts": "npm run sd-typeThemePtSans && npm run sd-typeThemeOpenMontserrat && npm run sd-typeThemePtSans && npm run sd-typeThemeProximaNova && npm run sd-typeThemeTenor && npm run sd-typeThemeVTB && npm run sd-typeThemeTimchenko && npm run sd-typeThemeTravel && npm run sd-typeThemeVK",
    "sd-themes": "npm run sd-generalThemes && npm run sd-colorThemeMinfin && npm run sd-colorThemeDarkGray && npm run sd-colorThemeMinfin && npm run sd-colorThemeBlue && npm run sd-colorThemeBlackGreen && npm run sd-colorThemeDefault && npm run sd-colorThemeAbsolute && npm run sd-colorThemeBrown && npm run sd-colorThemePink && npm run sd-colorThemeYellow && npm run sd-colorThemeRed && npm run sd-colorThemeTurquoise && npm run sd-colorThemeVTB && npm run sd-colorThemeTimchenko && npm run sd-colorThemeTravel && npm run sd-colorThemeVK",
    "sd-generalThemes": "npx token-transformer ./myrtex/tokens.json ./transformed/generalThemes.json general,generalColors,systemColors generalColors --expandShadow=true",
    "sd-colorThemeMinfin": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeMinfin.json brandMinfin,generalColors generalColors",
    "sd-colorThemeBlue": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeBlue.json brandBlue,generalColors generalColors",
    "sd-colorThemeBlackGreen": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeBlackGreen.json brandBlackGreen,generalColors generalColors",
    "sd-colorThemeDefault": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeDefault.json brandDefault,generalColors generalColors",
    "sd-colorThemeAbsolute": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeAbsolute.json brandAbsolute,generalColors generalColors",
    "sd-colorThemeBrown": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeBrown.json brandBrown,generalColors generalColors",
    "sd-colorThemePink": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemePink.json brandPink,generalColors generalColors",
    "sd-colorThemeYellow": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeYellow.json brandYellow,generalColors generalColors",
    "sd-colorThemeRed": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeRed.json brandRed,generalColors generalColors",
    "sd-colorThemeTurquoise": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeTurquoise.json brandTurquoise,generalColors generalColors",
    "sd-colorThemeVTB": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeVTB.json brandVtb,generalColors generalColors",
    "sd-colorThemeDarkGray": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeDarkGray.json brandDarkGray,generalColors generalColors",
    "sd-colorThemeTimchenko": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeTimchenko.json brandTimchenko,generalColors generalColors",
    "sd-colorThemeTravel": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeTravel.json brandTravel,generalColors generalColors",
    "sd-colorThemeVK": "npx token-transformer ./myrtex/tokens.json ./transformed/colorThemeVK.json brandVK,generalColors generalColors",
    "sd-typeThemeOpenMontserrat": "npx token-transformer ./myrtex/tokens.json ./transformed/typeThemeOpenMontserrat.json typeOpenMontserrat,generalType generalType --expandTypography=true",
    "sd-typeThemePtSans": "npx token-transformer ./myrtex/tokens.json ./transformed/typeThemePtSans.json typePtSans,generalType generalType --expandTypography=true",
    "sd-typeThemeProximaNova": "npx token-transformer ./myrtex/tokens.json ./transformed/typeThemeProximaNova.json typeProximaNova,generalType generalType --expandTypography=true",
    "sd-typeThemeTenor": "npx token-transformer ./myrtex/tokens.json ./transformed/typeThemeTenor.json typeTenor,generalType generalType --expandTypography=true",
    "sd-typeThemeVTB": "npx token-transformer ./myrtex/tokens.json ./transformed/typeThemeVTB.json typeVtb,generalType generalType --expandTypography=true",
    "sd-typeThemeTimchenko": "npx token-transformer ./myrtex/tokens.json ./transformed/typeThemeTimchenko.json typeTimchenko,generalType generalType --expandTypography=true",
    "sd-typeThemeTravel": "npx token-transformer ./myrtex/tokens.json ./transformed/typeThemeTravel.json typeTravel,generalType generalType --expandTypography=true",
    "sd-typeThemeVK": "npx token-transformer ./myrtex/tokens.json ./transformed/typeThemeVK.json typeVK,generalType generalType --expandTypography=true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "style-dictionary": "^3.7.1",
    "token-transformer": "^0.0.27"
  }
}
